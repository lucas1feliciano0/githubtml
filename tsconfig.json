{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "lib": [
      "es2017"
    ],
    "allowJs": true,
    "jsx": "react-native",
    "removeComments": true,
    "noEmit": true,
    "isolatedModules": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "types": [
      "styled-components-react-native"
    ],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": false,
    "baseUrl": "./",
    "paths": {
      "*": [
        "src/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@navigation/*": [
        "./src/navigation/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@util/*": [
        "./src/util/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@theme/*": [
        "./src/theme/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
  "extends": "expo/tsconfig.base"
}
